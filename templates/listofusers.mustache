{{!
    @template block_superframe/frame

    This template renders a list with the users of the course

    Example context (json):
    {
        "students": "array [[image,name]]",
    }
}}
<div>
    <ul class="list-group">
        {{#students}}
            <li class="list-group-item list-group-item-warning">{{{image}}}{{name}}</li>
        {{/students}}
    </ul>
</div>